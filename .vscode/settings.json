{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsc.autoDetect": "off",
  "json.schemas": [
    {
      "fileMatch": ["/*electron-builder.json5", "/*electron-builder.json"],
      "url": "https://json.schemastore.org/electron-builder"
    }
  ],
  "vetur.validation.script": false,
  "code-runner.runInTerminal": true,
  "files.associations": {
    "algorithm": "cpp",
    "iostream": "cpp",
    "ostream": "cpp",
    "vector": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "exception": "cpp",
    "initializer_list": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "istream": "cpp",
    "iterator": "cpp",
    "limits": "cpp",
    "memory": "cpp",
    "new": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "utility": "cpp",
    "xfacet": "cpp",
    "xiosbase": "cpp",
    "xlocale": "cpp",
    "xlocinfo": "cpp",
    "xlocnum": "cpp",
    "xmemory": "cpp",
    "xstddef": "cpp",
    "xstring": "cpp",
    "xtr1common": "cpp",
    "xutility": "cpp",
    "charconv": "cpp",
    "cstdarg": "cpp",
    "format": "cpp",
    "functional": "cpp",
    "list": "cpp",
    "locale": "cpp",
    "mutex": "cpp",
    "optional": "cpp",
    "ratio": "cpp",
    "stop_token": "cpp",
    "thread": "cpp",
    "unordered_map": "cpp",
    "xhash": "cpp",
    "xlocbuf": "cpp",
    "xlocmes": "cpp",
    "xlocmon": "cpp",
    "xloctime": "cpp",
    "chrono": "cpp",
    "forward_list": "cpp",
    "iomanip": "cpp"
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt -lpsapi -lgdi32 -luser32 -lversion && $dir$fileNameWithoutExt"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "cSpell.words": [
    "CHANGECBCHAIN",
    "CLIPBOARDUPDATE",
    "Dexie",
    "DRAWCLIPBOARD",
    "HDROP",
    "hwnd",
    "HWND",
    "LPARAM",
    "lpfn",
    "lpsz",
    "LRESULT",
    "mouseleave",
    "mousemove",
    "Napi",
    "OVERLAPPEDWINDOW",
    "pinia",
    "UNICODETEXT",
    "unwatch",
    "USEDEFAULT",
    "WNDCLASS",
    "WPARAM"
  ],
  "ti18n.shortcutLanguages": ["zh_CN", "en_US"]
}
